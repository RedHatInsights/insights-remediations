          become: false

- name: run insights
  hosts: \\"1f12bdfc-8267-492d-a930-92f498fe65b9.ansible.example.com,fc94beb8-21ee-403d-99b1-949ef7adb762\\"
  become: True
  gather_facts: False
  tasks:
    - name: run insights
