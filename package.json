{
  "name": "insights-remediations",
  "version": "0.1.0",
  "description": "Generator of automated remediations for Red Hat Insights",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:RedHatInsights/insights-remediations.git"
  },
  "scripts": {
    "coverage": "NODE_ENV=test npm run db:ims && jest -i --coverage",
    "db:ims": "npm-run-all db:init db:migrate db:seed",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:init": "sequelize db:drop; sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:migrate:undo:all": "sequelize db:migrate:undo:all",
    "db:seed": "sequelize db:seed:all",
    "lint": "npm-run-all lint:js lint:openapi",
    "lint:js": "eslint *.js db src",
    "lint:yaml": "yamllint src/api/openapi.yaml",
    "lint:openapi": "speccy lint src/api/openapi.yaml && node src/api/lint.js",
    "prod": "NODE_ENV=production node src/app.js",
    "start": "NODE_ENV=development nodemon --watch src  -e js,yaml src/app.js",
    "test": "NODE_ENV=test npm run db:ims && jest -i",
    "test:unit": "NODE_ENV=test jest -i --testMatch='**/?(*.)+(unit).js'",
    "test:contract": "NODE_ENV=test jest -i --testMatch='**/?(*.)+(contract).js'",
    "verify": "npm-run-all lint coverage"
  },
  "author": "Jozef Hartinger",
  "dependencies": {
    "@godaddy/terminus": "^4.4.1",
    "app-common-js": "^1.2.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cls-bluebird": "^2.1.0",
    "dedent-js": "^1.0.1",
    "etag": "^1.8.1",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "express-pino-logger": "^4.0.0",
    "express-prom-bundle": "^5.1.5",
    "graphql-request": "^1.8.2",
    "ioredis": "^4.19.2",
    "js-yaml": "^3.14.0",
    "jszip": "^3.5.0",
    "lodash": "^4.17.20",
    "mustache": "^3.2.1",
    "openapi-default-setter": "^2.1.0",
    "openapi-request-coercer": "^2.4.0",
    "openapi-request-validator": "^3.8.3",
    "openapi-response-validator": "^3.8.2",
    "openapi-schema-validator": "^3.0.3",
    "pg": "^7.18.2",
    "pino": "^6.7.0",
    "pino-cloudwatch": "^0.7.0",
    "pino-multi-stream": "^5.1.1",
    "prismjs": "^1.24.0",
    "prom-client": "^11.5.3",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^5.5.1",
    "swagger-ui-express": "^4.1.4",
    "urijs": "^1.19.5",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^25.5.4",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^4.3.0",
    "should": "^13.2.3",
    "sinon": "^7.5.0",
    "speccy": "^0.11.0",
    "supertest": "^4.0.2"
  }
}
